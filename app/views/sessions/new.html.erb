<body class= "session_new_body" >
  <h1 class="session_new_h1">ログイン</h1>
  <div class="session_new_div">
    <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
      <%= f.label :email, "メールアドレス" %>
      <%= f.email_field :email %>
      <%= f.label :password %>
      <%= f.password_field :password %>
      <%= f.submit "ログイン" %>
    <% end %>
  </div>
</body>
